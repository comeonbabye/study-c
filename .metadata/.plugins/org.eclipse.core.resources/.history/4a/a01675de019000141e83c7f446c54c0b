#include <stdio.h>    //stdio 就是指 “standard input & output"（标准输入输出）
#include <stdlib.h>

int main(void) {  //main函数

	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */

	printf("hello tony.");

	return EXIT_SUCCESS;   //返回执行成功
}
